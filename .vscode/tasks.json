{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "CMake: build",
      "type": "shell",
      "command": "bash",
      "args": [
        "-lc",
        "cmake -S . -B build -DCMAKE_BUILD_TYPE=Debug && cmake --build build --config Debug -- -j"
      ],
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
